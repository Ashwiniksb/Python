   "source": [
    "#To find factorial of a number\n",
    "def factorial(x):\n",
    "    if x < 0:\n",
    "        return \"Not allowed\"\n",
    "    elif x == 0 | x == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return x * factorial(x-1)\n",
    "print(factorial())"
   ]
